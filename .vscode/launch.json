  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Debug k6 test file",
      "runtimeExecutable": "k6",
      "runtimeArgs": ["run", "${file}"],
      "outputCapture": "std",
      "env": {
        "NODE_PATH": "src:test"
      },
      "skipFiles": [
        "<node_internals>/**/*.js",
        "${workspaceFolder}/node_modules/**/*.js"
      ]
    }
  ]
}
